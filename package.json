{
  "name": "Nebula",
  "version": "0.0.1",
  "description": "The IPFS client geared towards ease-of-use.",
  "author": "Cubefuse",
  "license": "MIT",
  "homepage": "https://github.com/cubefuse/nebula",
  "repository": "github:cubefuse/nebula",
  "main": "index.html",
  "scripts": {
    "prestart": "flow",
    "start": "webpack-dev-server --config ./webpack.dev.js --mode development",
    "build": "webpack --config ./webpack.prod.js --mode production",
    "test": "jest",
    "electron": "electron ./src/desktop.js",
    "electron-toolkit": "electron ./node_modules/electron-toolkit",
    "compile": "electron-builder",
    "precommit": "pretty-quick && jest",
    "flow": "flow",
    "docs": "esdoc",
    "pretty": "pretty-quick"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-toolkit": "^1.0.24",
    "esdoc": "^1.1.0",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "flow-bin": "^0.74.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jest-dom": "^1.3.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "prettier": "1.13.5",
    "pretty-quick": "^1.6.0",
    "react-hot-loader": "^4.0.0",
    "react-testing-library": "^3.1.4",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "antd": "^3.6.2",
    "debug": "^3.1.0",
    "ipfs": "^0.29.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "build": {
    "appId": "com.electron.nebula",
    "copyright": "Copyright Â© 2018 Cubefuse",
    "productName": "Nebula",
    "electronVersion": "2.0.2",
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
